---
const navLinks = [
  { href: "/", label: "Home" },
  { href: "/about", label: "About" },
  { href: "/services", label: "Services" },
  { href: "/contact", label: "Contact" }
];
const logoSrc = "/assets/ico/logo.png";
---
<section
      id="navbar"
      aria-label="Main Navigation"
      class="relative z-[999] flex min-h-16 w-full items-center  bg-highlight-25 px-[5%] md:min-h-18"
    >
<nav class="bg-gray-900 text-white px-4 py-3">
  <div class="container mx-auto flex items-center justify-between">
    <a href="/" class="flex items-center">
      <img src={logoSrc} alt="Logo" class="h-8 mr-2" />
      <span class="font-bold text-lg">Your Company</span>
    </a>
    <ul class="flex space-x-6">
      {navLinks.map(link => (
        <li>
          <a href={link.href} class="hover:text-gray-400">{link.label}</a>
        </li>
      ))}
    </ul>
  </div>
</nav>
</section>